                "id": "T002tool",
                "template": "MATCH (f:entity) MATCH (f)-[:$REL_TYPE]->(provider:entity) WHERE ANY(c IN provider.concept WHERE toLower(c) CONTAINS 'tool') RETURN f.id AS TargetFunction, count(provider) AS CountOfCodeProviders,collect(provider.id) AS ListOfCodeProviders",
                "parameters": {
                    "PROP_ID": "string",
                    "VALUE": "string",
                    "REL_TYPE": "string"
                }
            }
        ]

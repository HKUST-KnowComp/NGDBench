    {
        "type": "basic_expand_aggregation",
        "templates": [
            {
                "id": "T001",
                "template": "MATCH (n:entity {$PROP_ID: $VALUE})-[:$REL_TYPE]->(m) RETURN count(m) AS cnt, collect(m) AS m_list",
                "parameters": {
                    "PROP_ID": "string",
                    "VALUE": "string",
                    "REL_TYPE": "string"
                }
            },
            {
                "id": "T002",
                "template": "MATCH (n:$LABEL {$PROP: $VALUE})-[:$REL_TYPE]->(m:$L2) RETURN avg(m.$PROP2) AS avg_value",
                "parameters": {
                    "LABEL": "string",
                    "PROP": "string",
                    "VALUE": "string",
                    "REL_TYPE": "string",
                    "L2": "string",
                    "PROP2": "string"
                }
            },
            {
                "id": "T003",
                "template": "MATCH (n:$LABEL {$PROP: $VALUE})-[:$REL_TYPE]->(m) RETURN sum(m.$PROP2) AS total",
                "parameters": {
                    "LABEL": "string",
                    "PROP": "string",
                    "VALUE": "string",
                    "REL_TYPE": "string",
                    "PROP2": "string"
                }
            }
        ]
    },
    {
        "type": "basic_expand_into_aggregation",
        "templates": [
            {
                "id": "T001",
                "template": "MATCH (a:entity {$PROP_ID: $VALUE})-[:$REL1]->(b:$LABEL2) MATCH (a)-[:$REL2]->(b) RETURN count(*) AS cnt",
                "parameters": {
                    "PROP_ID": "string",
                    "VALUE": "string",
                    "REL1": "string",
                    "REL2": "string"
                }
            },
            {
                "id": "T002",
                "template": "MATCH (a:entity {$PROP_ID: $VALUE})-[:$REL1]->(b:entity) MATCH (a)-[:$REL2]->(b) RETURN a, count(b) AS cnt",
                "parameters": {
                    "PROP_ID": "string",
                    "VALUE": "string",
                    "REL1": "string",
                    "REL2": "string"
                }
            }
        ]
    },
    {
        "type": "basic_filter_aggregation",
        "templates": [
            {
                "id": "T001",
                "template": "MATCH (a:entity {$PROP_ID: $VALUE})-[r:$REL_TYPE]-(b:entity) WHERE r.$REL_PROP $OP $VALUE RETURN count(a) AS cnt",
                "parameters": {
                    "PROP_ID": "string",
                    "VALUE": "string",
                    "REL_TYPE": "string",
                    "REL_PROP": "string",
                    "OP": "string"
                }
            },
            {
                "id": "T002",
                "template": "MATCH (a:entity {$PROP_ID: $VALUE})-[r:$REL_TYPE]-(b:entity) WHERE r.$REL_PROP $OP $VALUE RETURN a, count(b) AS cnt",
                "parameters": {
                    "PROP_ID": "string",
                    "VALUE": "string",
                    "REL_TYPE": "string",
                    "REL_PROP": "string",
                    "OP": "string"
                }
            },
            {
                "id": "T003",
                "template": "MATCH (a:entity {$PROP_ID: $VALUE})-[r:$REL_TYPE]-(b:entity) WHERE r.$REL_PROP $OP $VALUE RETURN avg(r.$REL_PROP), max(r.$REL_PROP)",
                "parameters": {
                    "PROP_ID": "string",
                    "VALUE": "string",
                    "REL_TYPE": "string",
                    "REL_PROP": "string",
                    "OP": "string"
                }
            }
        ]
    },
    {
        "type": "nested_loop",
        "templates": [
            {
                "id": "T002",
                "template": "MATCH (a:entity) WHERE (a)-[:$REL_TYPE]->(:entity {$PROP_ID: $VALUE}) RETURN a.id",
                "parameters": {
                    "REL_TYPE": "string",
                    "PROP_ID": "string",
                    "VALUE": "string"
                }
            },
            {
                "id": "T005",
                "template": "MATCH (n:entity) WHERE (n)-[:$REL1]->(:entity {$PROP_ID: $VALUE}) RETURN n.id",
                "parameters": {
                    "REL1": "string",
                    "PROP_ID": "string",
                    "VALUE": "string"
                }
            },
            {
                "id": "T011",
                "template": "MATCH (a:entity {$PROP_ID: $VALUE}), (b:$LABEL2) WHERE (a)-[:$REL_TYPE]->(b) RETURN count(b),collect(b) AS b_list",
                "parameters": {
                    "PROP_ID": "string",
                    "VALUE": "string",
                    "REL_TYPE": "string"
                }

            },
            {
                "id": "T012",
                "template": "MATCH (n:entity) WHERE (n)-[:$REL1]->(:entity {$PROP_ID1: $VALUE1}) OR (n)-[:$REL2]->(:entity {$PROP_ID2: $VALUE2}) RETURN count(n) AS cnt,collect(n) AS n_list",
                "parameters": {
                    "REL1": "string",
                    "REL2": "string",
                    "PROP_ID1": "string",
                    "VALUE1": "string",
                    "PROP_ID2": "string",
                    "VALUE2": "string"
                }
            }
        ]
    },
    {
        "type": "chain",
        "templates": [
            {
                "id": "T001",
                "template": "MATCH (a:entity {$PROP_ID1: $VALUE})-[:$REL1]->(b:entity)-[:$REL2]->(c:entity)-[:$REL3]->(d:entity) RETURN a.id, b.id, c.id, d.id",
                "parameters": {
                    "PROP_ID1": "string",
                    "VALUE": "string",
                    "REL1": "string",
                    "REL2": "string",
                    "REL3": "string"
                }
            },
            {
                "id": "T002",
                "template": "MATCH (a:entity {$PROP_ID1: $VALUE1})-[:$REL_TYPE *$MIN_HOPS..$MAX_HOPS]-(b:entity) RETURN a.id, b.id",
                "parameters": {
                    "PROP_ID1": "string",
                    "VALUE1": "string",
                    "REL_TYPE": "string",
                    "MIN_HOPS": "integer",
                    "MAX_HOPS": "integer"
                }
            },
            {
                "id": "T005",
                "template": "MATCH (a:entity {$PROP_ID1: $VALUE})-[:$REL1]->(b:entity)-[:$REL2]->(c:entity)-[:$REL3]->(d:entity) RETURN a.id, count(d) AS cnt,collect(d) AS d_list",
                "parameters": {
                    "PROP_ID1": "string",
                    "VALUE": "string",
                    "REL1": "string",
                    "REL2": "string",
                    "REL3": "string"
                }
            },
            {
                "id": "T010",
                "template": "MATCH p = (a:entity {$PROP_ID: $VALUE})-[:$REL_TYPE *$MIN_HOPS..$MAX_HOPS]-(b:entity) RETURN avg(length(p)) AS avg_len",
                "parameters": {
                    "PROP_ID": "string",
                    "VALUE": "string",
                    "REL_TYPE": "string",
                    "MIN_HOPS": "integer",
                    "MAX_HOPS": "integer"
                }
            }
        ]
    },
    {
        "type": "join",
        "templates": [
            {
                "id": "T001",
                "template": "MATCH (a:$LABEL1) WITH a ORDER BY a.$PROP1 LIMIT 10 MATCH (b:$LABEL2) WITH a, b ORDER BY b.$PROP2 LIMIT 10 RETURN a, b",
                "parameters": {
                    "LABEL1": "string",
                    "LABEL2": "string",
                    "PROP1": "string",
                    "PROP2": "string"
                }
            },
            {
                "id": "T003",
                "template": "MATCH (a:$LABEL1) WITH a ORDER BY a.$PROP1 MATCH (b:$LABEL2) WHERE a <> b WITH a, b ORDER BY b.$PROP2 RETURN count(*) AS pair_count",
                "parameters": {
                    "LABEL1": "string",
                    "LABEL2": "string",
                    "PROP1": "string",
                    "PROP2": "string"
                }
            },
            {
                "id": "T004",
                "template": "MATCH (a:$LABEL1), (b:$LABEL2) WHERE a.$PROP1 = b.$PROP2 AND a <> b RETURN count(*) AS cnt",
                "parameters": {
                    "LABEL1": "string",
                    "LABEL2": "string",
                    "PROP1": "string",
                    "PROP2": "string"
                }
            },
            {
                "id": "T006",
                "template": "MATCH (a:$LABEL1), (b:$LABEL2) WHERE a.$PROP1 = b.$PROP2 AND a <> b WITH a, count(b) AS cnt WHERE cnt > $THRESHOLD RETURN a",
                "parameters": {
                    "LABEL1": "string",
                    "LABEL2": "string",
                    "PROP1": "string",
                    "PROP2": "string",
                    "THRESHOLD": "integer"
                }
            }
        ]
    },
    {
        "type": "pipelined",
        "templates": [
            {
                "id": "T001",
                "template": "MATCH (a:entity {$PROP_ID: $VALUE})-[:$REL1]->(b:entity) WITH b MATCH (b)-[:$REL2]->(c:entity) WHERE c.$FILTER_PROP = $FILTER_VAL RETURN b.id, c.id",
                "parameters": {
                    "PROP_ID": "string",
                    "VALUE": "string",
                    "REL1": "string",
                    "REL2": "string",
                    "FILTER_PROP": "string",
                    "FILTER_VAL": "string",
                    "RET_PROP1": "string",
                    "RET_PROP2": "string",
                    "RET_PROP3": "string"
                }
            },
            {
                "id": "T003",
                "template": "MATCH (a:entity {$PROP_ID: $VALUE})-[:$REL1]->(b:entity) WITH b MATCH (b)-[:$REL2]->(c:entity) WHERE c.$FILTER_PROP = $FILTER_VAL RETURN b, count(c) AS cnt",
                "parameters": {
                    "PROP_ID": "string",
                    "VALUE": "string",
                    "REL1": "string",
                    "REL2": "string",
                    "FILTER_PROP": "string",
                    "FILTER_VAL": "string"
                }
            },
            {
                "id": "T004",
                "template": "MATCH (a:entity {$PROP_ID: $VALUE})-[:$REL1]->(b:entity) WITH b MATCH (b)-[:$REL2]->(c:entity) WHERE c.$FILTER_PROP = $FILTER_VAL WITH b, count(c) AS cnt WHERE cnt > $THRESHOLD RETURN b, cnt",
                "parameters": {
                    "LABEL1": "string",
                    "LABEL2": "string",
                    "LABEL3": "string",
                    "PROP1": "string",
                    "VALUE": "string",
                    "REL1": "string",
                    "REL2": "string",
                    "FILTER_PROP": "string",
                    "FILTER_VAL": "string",
                    "THRESHOLD": "integer"
                }
            }
        ]
    },
    {
        "type": "triadic",
        "templates": [
            {
                "id": "T002",
                "template": "MATCH (a:entity {$PROP_ID: $VALUE})-[:$REL]-()-[:$REL]-(b:entity) WHERE NOT (a)-[:$REL]-(b) WITH a, count(DISTINCT b) AS cnt RETURN a.id, cnt",
                "parameters": {
                    "PROP_ID": "string",
                    "VALUE": "string",
                    "REL": "string"
                }
            }
        ]
    }
]
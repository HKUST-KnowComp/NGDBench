        "query_template": "MATCH (dis1:Disease {name: \"{disease_A}\"})<-[:indication]-(d:Drug)-[:contraindication]->(dis2:Disease {name: \"{disease_B}\"}) RETURN d.name AS DrugName",
        "query_parameters": {

{
    "noise_types": {
      "false_edges": {
        "description": "Replace either x_id or y_id with another valid ID of the same node type to simulate incorrect biomedical relations extracted from noisy sources.",
        "target_fields": ["x_id", "y_id"],
        "refer_fields": ["x_type", "y_type"],
        "constraints": {
          "type_consistent": true,
          "avoid_existing_edges": true
        }
      },
      "relation_type_noise": {
        "description": "Replace or add the relation type or display_relation with another valid relation of the same semantic family to simulate extraction misclassification.",
        "target_fields": ["relation", "display_relation"],
        "constraints": {
          "same_relation_family": true
        }
      },
      "name_typos": {
        "description": "Inject character-level noise into entity names to mimic OCR errors, NLP extraction mistakes, or synonym ambiguity.",
        "target_fields": ["x_name", "y_name"],
        "operations": ["character_substitution", "character_deletion", "character_insertion", "case_alteration", "synonym_replacement"]
      },
      "source_conflicts": {
        "description": "Manipulate source information or inject conflicting edges from other typical biomedical databases.",
        "target_fields": ["x_source", "y_source"],
        "operations": ["conflicting_relation_injection"]
      },
      "node_type_noise": {
        "description": "Replace node types (e.g., gene/protein, drug, biological_process) to simulate entity classification errors.",
        "target_fields": ["x_type", "y_type"],
        "constraints": {
          "allow_invalid_combinations": true
        }
      },
      "id_corruption": {
        "description": "Corrupt IDs (e.g., DrugBank, NCBI) by truncating, replacing characters, removing prefixes, or generating invalid IDs.",
        "target_fields": ["x_id", "y_id"],
        "operations": ["truncate", "character_swap", "prefix_replacement", "random_id"]
      },
      "duplicate_edges": {
        "description": "Duplicate existing edges or inject contradictory versions to mimic merging of heterogeneous data sources.",
        "target_fields": ["row"],
        "operations": ["duplicate", "contradict_relation"]
      },
      "path_level_noise": {
        "description": "Replace intermediate nodes in multi-hop biomedical reasoning chains to simulate incorrect causal or mechanistic links.",
        "target_fields": ["x_id", "y_id"],
        "constraints": {
          "structural_consistency": false
        }
      }
    },
  
    "default_profile": {
      "false_edges": 0.15,
      "missing_edges": 0.05,
      "name_typos": 0.05,
      "relation_type_noise": 0.08,
      "source_conflicts": 0.05,
      "node_type_noise": 0.04,
      "id_corruption": 0.03,
      "duplicate_edges": 0.10,
      "path_level_noise": 0.05
    },
  
    "field_targets": {
      "relation_fields": ["relation", "display_relation"],
      "entity_fields": ["x_name", "y_name"],
      "id_fields": ["x_id", "y_id"],
      "type_fields": ["x_type", "y_type"],
      "source_fields": ["x_source", "y_source"],
      "row_level": ["row"]
    },
  
    "relation_families": {
      "protein_protein": ["ppi", "binds", "interacts_with", "associates_with"],
      "bioprocess_protein": ["interacts_with", "positively_regulates", "negatively_regulates"],
      "drug_drug": ["synergistic_interaction", "antagonistic_interaction", "coadministration"]
    },
  
    "id_formats": {
      "DrugBank": "^DB\\d{5}$",
      "NCBI_gene": "^\\d+$",
      "GO_process": "^\\d{4,6}$"
    },
  
    "examples": {
      "false_edge_example": {
        "original": ["protein_protein", "ppi", "9796", "56992"],
        "noisy": ["protein_protein", "ppi", "9796", "12345"]
      },
      "name_typo_example": {
        "original": "Testosterone enanthate",
        "noisy": "Testosteron enanthte"
      },
      "relation_type_example": {
        "original": "synergistic interaction",
        "noisy": "antagonistic interaction"
      },
      "id_corruption_example": {
        "original": "DB01380",
        "noisy": "DB1380"
      }
    }
  }
  